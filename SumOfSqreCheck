m = int(input("Enter the value of m: "))
n = int(input("Enter the value of n: "))

number = 10**m + 10**n

found = False
limit = int(number**0.5) + 1

for i in range(limit):
    j2 = number - i*i
    if j2 < 0:
        break
    j = int(j2**0.5)
    if i*i + j*j == number:
        print(f"{number} = {i}^2 + {j}^2")
        found = True
        break

if not found:
    print(f"{number} cannot be written as sum of two squares")
